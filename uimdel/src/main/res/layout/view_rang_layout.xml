<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:minHeight="@dimen/rang_min_height"
    tools:parentTag="android.widget.FrameLayout">

    <ImageView
        android:id="@+id/middle"
        android:layout_width="@dimen/rang_with"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/rang_middle_top"
        android:backgroundTint="@color/storage_pie_mask"
        android:layout_marginBottom="@dimen/rang_bottom_height"
        tools:background="@drawable/strorage_column_m_0"/>

    <ImageView
        android:id="@+id/top"
        android:layout_width="@dimen/rang_with"
        android:layout_height="@dimen/rang_top_height"
        android:backgroundTint="@color/storage_pie_mask"
        tools:src="@drawable/strorage_column_t_0"/>

    <ImageView
        android:id="@+id/bottom"
        android:layout_width="@dimen/rang_with"
        android:layout_height="@dimen/rang_bottom_height"
        android:backgroundTint="@color/storage_pie_mask"
        tools:src="@drawable/strorage_column_b_0"
        android:layout_gravity="bottom"/>
</merge>