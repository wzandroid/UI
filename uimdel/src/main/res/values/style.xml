<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="rangStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/rang_min_height</item>
        <item name="layout_constraintStart_toStartOf">@id/group</item>
        <item name="layout_constraintEnd_toEndOf">@id/group</item>
    </style>

    <style name="storage_space">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginTop">@dimen/rang_top_height</item>
    </style>

    <style name="RangTitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layout_constraintStart_toEndOf">@id/group</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginStart">@dimen/rang_title_ms</item>
    </style>

    <style name="RangSizeStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
    </style>
</resources>